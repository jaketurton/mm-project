ay: flex;\n        flex-direction: row;\n        justify-content: center;\n        text-align: center;\n    }\n\n    &__indicator {\n        font-size: 3em;\n        color: #fff;\n        background: none;\n        border: none;\n        font-weight: 100;\n\n        &--active {\n            font-weight: 900;\n        }\n    }\n\n    &__prev,\n    &__next {\n        z-index: 3;\n        position: absolute;\n        top: 25%;\n\n        @media (min-width: $breakpoint-two-columns) {\n            top: 40%;\n        }\n\n        * {\n            background: none;\n            border: 0;\n        }\n\n        button {\n            padding: 1em;\n            height: 80px;\n            width: 80px;\n        }\n\n        svg {\n            width: 100%;\n            fill: white;\n            height: 45px;\n            width: 45px;\n        }\n    }\n\n    &__prev {\n        left: 0;\n        transform: rotate(180deg);\n        \n        @media (min-width: $breakpoint-two-columns) {\n            left: 1em;\n        }\n    }\n\n    &__next {\n        right: 0;\n\n        @media (min-width: $breakpoint-two-columns) {\n            right: 1em;\n        }\n    }\n\n    &__slide {\n        height: 90vh;\n        display: flex;\n        flex-wrap: wrap;\n        flex-direction: column;\n        width: 100%;\n        position: absolute;\n        top: 0;\n        left: 0;\n        bottom: 0;\n        right: 0;\n        transition: opacity 1s ease;\n        z-index: 1;\n        opacity: 0;\n\n        &--visible {\n            opacity: 1;\n            z-index: 2;\n        }\n\n        @media (min-width: $breakpoint-two-columns) {\n            flex-direction: row;\n        }\n    }\n\n    &__logo-container {\n        position: absolute;\n        top: 2em;\n        left: 2em;\n        z-index: 10;\n    }\n\n    &__logo {\n        width: 100px;\n        height: 80px;\n    }\n\n    &__nav-container {\n        position: absolute;\n        top: 2em;\n        right: 2em;\n        z-index: 3;\n        background-color: none;\n        transition: background-color 0s;\n\n        &--modal {\n            position: fixed;\n            left: 0;\n            top: 0;\n            right: 0;\n            bottom: 0;\n            z-index: 4;\n            background-color: $mm-black;\n            transition: background-color 0.5s ease;\n\n            .hero__nav-button {\n                display: none;\n            }\n\n            .hero__nav-list {\n                font-size: 2em;\n                color: #fff;\n                display: flex;\n                flex-direction: column;\n                align-content: center;\n                justify-content: center;\n                text-align: center;\n                margin-top: 40%;\n                opacity: 1;\n                transition: opacity 0.5s ease;\n            }\n        }\n    }\n\n    &__nav-button {\n        border: 0;\n        width: 60px;\n        height: 60px;\n        background: none;\n\n        @media (min-width: $breakpoint-two-columns) {\n            display: none;\n        }\n    }\n\n    &__nav-list {\n        display: none;\n        opacity: 0;\n        color: #fff;\n        font-weight: bold;\n\n        @media (min-width: $breakpoint-two-columns) {\n            display: flex;\n            opacity: 1;\n\n            li {\n                margin-left: 1em;\n            }\n        }\n    }\n\n    &__img-container\n    {\n        background-color: #fff;\n        height: 50%;\n        width: 100%;\n        background-size: cover;\n        background-position: 30%;\n        \n        @media (min-width: $breakpoint-two-columns) {\n            height: 100%;\n            width: 50%;\n            order: 2;\n            background-position: 60%;\n        }\n    }\n\n    &__text-container\n    {\n        background: $mm-cyan;\n        height: 50%;\n        width: 100%;\n        position: relative;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n\n        @media (min-width: $breakpoint-two-columns) {\n            height: 100%;\n            width: 50%;\n            order: 1;\n        }\n    }\n\n    &__text {\n        max-width: 400px;\n        padding: 0 2em;\n\n        @media (min-width: $breakpoint-two-columns) {\n            max-width: 300px;\n            padding: 0;\n        }\n    }\n\n    &__header {\n        color: #fff;\n        font-size: 1.9em;\n        margin-bottom: 0.4em;\n        line-height: 1.2;\n\n        @media (min-width: $breakpoint-two-columns) {\n            font-size: 3.5em;\n        }\n    }\n\n    &__arrow-container\n    {\n        background: #fff;\n        height: 10vh;\n        width: 100%;\n        order: 3;\n        display: flex;\n        align-items: center;\n        z-index: 3;\n    }\n\n    &__arrow {\n        width: 65px;\n        height: 65px;\n        display: block;\n        margin: 0 auto;\n        background: none;\n        border: 0;\n    }\n}\n\n.container {\n    width: 100%;\n    padding: 2em;\n\n    @media (min-width: $breakpoint-two-columns) { \n        max-width: 1020px;\n        margin: 0 auto;\n    }\n}\n\n.paragraph {\n    font-size: 1em;\n    color: $mm-black;\n    margin-bottom: 0.5em;\n    \n    &--invert {\n        color: #fff;\n    }\n}\n\n.header {\n    &__primary {\n        color: $mm-cyan;\n        font-weight: bold;\n        font-size: 1.8em;\n        margin-bottom: 0.5em;\n    }\n    &__secondary {\n        color: $mm-black;\n        font-weight: normal;\n        font-size: 1.4em;\n        margin-bottom: 0.5em;\n    }\n}\n\n.about {\n    background-color: #efefef;\n    overflow-x: hidden;\n    \n    @media (min-width: $breakpoint-two-columns) {\n        background-color: #fff;\n    }\n\n    &__content {\n        display: block;\n        opacity: 0;\n\n        @media (min-width: $breakpoint-two-columns) {\n            display: flex;\n\n            &:first-child {\n                margin-bottom: 3em;\n            }\n        }\n\n        &--inview {\n            opacity: 1;\n            transition: opacity 1s ease;\n        }\n\n        &--invert {\n            flex-direction: row-reverse;\n        }\n    }\n\n    &__text {\n        @media (min-width: $breakpoint-two-columns) {\n            padding: 2em;\n        }\n    }\n\n    &__image {\n        width: 100vw;\n        height: auto;\n        margin: 2em 0 2em -2em;\n        max-width: 700px;\n        display: block;\n        \n        @media (min-width: 720px) {\n            margin: 2em auto 2em auto;\n        }\n    \n        @media (min-width: $breakpoint-two-columns) { \n            margin: 0;\n            width: 100%;\n            height: 100%;\n            \n\n            &--right {\n                margin-right: -20%;\n            }\n    \n            &--left {\n                margin-left: -20%;\n            }\n        }\n    }\n}\n\n.footer {\n    background-color: #fff;\n    box-shadow: 0px 0px 10px rgba(0,0,0,0.3);\n    font-weight: bold;\n\n    @media (min-width: $breakpoint-two-columns) {\n        display: flex;\n    }\n\n    &__copyright-container {\n        margin-bottom: 0.5em;\n\n        @media (min-width: $breakpoint-two-columns) {\n            position: absolute;\n            margin-bottom: 0;\n        }\n    }\n\n    &__links-container {\n        li {\n            margin-bottom: 0.5em;\n        }\n\n        @media (min-width: $breakpoint-two-columns) {\n            text-align: center;\n            display: flex;\n            justify-content: center;\n\n            li {\n                margin-right: 1em;\n                margin-bottom: 0;\n\n                &:last-child {\n                    margin-right: 0;\n                }\n            }\n        }\n    }\n\n    &__link {\n        color: $mm-cyan;\n    }\n}"},"lineCount":null}},"error":null,"hash":"50bb349160cf4942d056b0788832745b","cacheData":{"env":{}}}